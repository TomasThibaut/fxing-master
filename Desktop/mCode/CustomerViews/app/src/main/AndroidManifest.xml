<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.gc.customerviews" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".slide_delete.SlideDelete"
            android:label="@string/title_activity_slide_delete_view" >
        </activity>
        <activity
            android:name=".toggle_button.ToggleBtnActivity"
            android:label="@string/title_activity_toggle_btn" >
        </activity>
    </application>

</manifest>
